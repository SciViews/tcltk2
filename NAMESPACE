S3method(print, tclTask)
S3method(tip, tk2widget)
S3method("tip<-", tk2widget)
S3method(print, tk2widget)
S3method(print, tk2cfglist)
S3method(state, tk2widget)
S3method(label, tk2widget)
S3method("label<-", tk2widget)
